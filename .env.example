# =============================================================================
# .env.example - AtBench AI Services Configuration
# =============================================================================
# Copy this file to .env and fill in your values
# Usage: cp .env.example .env
# =============================================================================

# Version Configuration
AI_VERSION=v1.3.1
INTERVIEW_VERSION=v1.3.2

# Docker Registry (optional)
DOCKER_REGISTRY=atbencher

# =============================================================================
# AI Provider API Keys
# =============================================================================
OPENAI_API_KEY=
GOOGLE_API_KEY=
DEEPSEEK_API_KEY=

# =============================================================================
# AWS S3 Configuration (for file storage)
# =============================================================================
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=eu-west-3
AWS_S3_BUCKET_NAME=atbench-resumes

# =============================================================================
# Database (optional - for some AI features)
# =============================================================================
DATABASE_URL=postgresql://postgres:postgres123@postgres:5432/recruiting_db

# =============================================================================
# Qdrant Vector Database
# =============================================================================
# For local Qdrant (default)
QDRANT_HOST=http://qdrant:6333
QDRANT_USE_HTTPS=false

# For external Qdrant (uncomment if using external)
# QDRANT_HOST=https://your-qdrant-host.com
# QDRANT_USE_HTTPS=true

QDRANT_API_KEY=your-qdrant-api-key
QDRANT_COLLECTION_NAME=resumes
QDRANT_OLLAMA_COLLECTION_NAME=ollama_resumes

# =============================================================================
# Ollama LLM Configuration
# =============================================================================
# Ollama runs on VPS host (installed via package, not Docker)
# Use VPS IP address for Docker containers to access
OLLAMA_HOST=http://72.60.200.75:11434

# For external Ollama Cloud (alternative)
# OLLAMA_HOST=https://api.ollama.cloud

OLLAMA_API_KEY=your-ollama-api-key
OLLAMA_MODEL=deepseek-v3.1:671b-cloud
OLLAMA_EXTRACTION_MODEL=deepseek-v3.1:671b-cloud
OLLAMA_TECHNICAL_MODEL=qwen3-coder:480b-cloud
OLLAMA_COMPLEX_MODEL=kimi-k2:1t-cloud
OLLAMA_FALLBACK_MODEL=qwen3-coder:480b-cloud
OLLAMA_FAST_MODEL=qwen3:8b

# =============================================================================
# Redis Configuration
# =============================================================================
REDIS_URL=redis://redis:6379

# =============================================================================
# NestJS API Configuration (for webhook callbacks)
# =============================================================================
# URL of the NestJS API for facial verification status updates
# This is where the interview service sends webhook callbacks after verification completes
NESTJS_API_URL=https://ai.atbench.com

# =============================================================================
# CORS Configuration
# =============================================================================
CORS_ORIGINS=https://atbench.com,https://www.atbench.com,http://localhost:3000,http://localhost
